{
    "name": "mobly/hour-bank",
    "description": "Parses hour bank e-mail and post on slack to a development team",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Caio Costa",
            "email": "github@bycosta.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "seleniumhq/selenium-server-standalone",
                "version": "2.48.2",
                "dist": {
                    "url": "https://selenium-release.storage.googleapis.com/2.48/selenium-server-standalone-2.48.2.jar",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "chromedriver/chromedriver-mac",
                "version": "2.20",
                "dist": {
                    "url": "http://chromedriver.storage.googleapis.com/2.20/chromedriver_mac32.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "chromedriver/chromedriver-linux",
                "version": "2.25",
                "dist": {
                    "url": "https://chromedriver.storage.googleapis.com/2.25/chromedriver_linux64.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "google/apiclient": "1.*",
        "box/spout": "~2.0",
        "maknz/slack": "~1.0",
        "mobly/simple-helpers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit-selenium": "dev-master",
        "seleniumhq/selenium-server-standalone": "2.48.2",
        "chromedriver/chromedriver-mac": "2.20",
        "chromedriver/chromedriver-linux": "2.25"
    }
}
